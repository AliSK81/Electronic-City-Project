<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<TableView xmlns:fx="http://javafx.com/fxml/1" fx:id="citiesTable" HBox.hgrow="ALWAYS"
           xmlns="http://javafx.com/javafx/16" fx:controller="Main.controller.Tables.CitiesTable">
    <columns>
        <TableColumn fx:id="nameCOL" text="City Name"/>
        <TableColumn fx:id="populationCOL" text="Population"/>
        <TableColumn fx:id="totalMoneyCOL" text="Total Money"/>
        <TableColumn fx:id="numTerminalsCOL" text="Terminals"/>
        <TableColumn fx:id="numHotelsCOL" text="Hotels"/>
        <TableColumn fx:id="numBanksCOL" text="Banks"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>
