<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<TableView xmlns:fx="http://javafx.com/fxml/1" fx:id="passengerPlaneTable" xmlns="http://javafx.com/javafx/16"
           fx:controller="Main.controller.Tables.PassengerPlanesTable" HBox.hgrow="ALWAYS">
    <columns>
        <TableColumn fx:id="idCOL" prefWidth="75.0" text="ID"/>
        <TableColumn fx:id="companyCOL" prefWidth="75.0" text="Company"/>
        <TableColumn fx:id="capacityCOL" prefWidth="75.0" text="Capacity"/>
        <TableColumn fx:id="priceCOL" prefWidth="75.0" text="Price"/>
        <TableColumn fx:id="heightCOL" prefWidth="98.0" text="Flight Height"/>
        <TableColumn fx:id="lengthCOL" prefWidth="88.0" text="Band Length"/>
        <TableColumn fx:id="seatsCOL" prefWidth="88.0" text="Seats"/>
        <TableColumn fx:id="numCrewsCOL" prefWidth="88.0" text="Crews"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>
