<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<TableView xmlns:fx="http://javafx.com/fxml/1" fx:id="busesTable" HBox.hgrow="ALWAYS"
           xmlns="http://javafx.com/javafx/16" fx:controller="Main.controller.Tables.BusesTable">
    <columns>
        <TableColumn fx:id="idCOL" prefWidth="75.0" text="ID"/>
        <TableColumn fx:id="companyCOL" prefWidth="75.0" text="Company"/>
        <TableColumn fx:id="capacityCOL" prefWidth="75.0" text="Capacity"/>
        <TableColumn fx:id="priceCOL" prefWidth="75.0" text="Price"/>
        <TableColumn fx:id="speedCOL" prefWidth="98.0" text="Speed"/>
        <TableColumn fx:id="lpkCOL" prefWidth="88.0" text="L.P.K"/>
        <TableColumn fx:id="yearCOL" prefWidth="88.0" text="Product Year"/>
        <TableColumn fx:id="lineCOL" prefWidth="88.0" text="Line"/>
        <TableColumn fx:id="vipCOL" prefWidth="88.0" text="VIP"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>
